package factory;

import java.util.LinkedList;
import java.util.List;

import model.Droid;
import model.Fighter;

public class TeamOfDroids {
	Droid firstDroid = new Fighter("First", 10, 10, 2, 1, true);
	Droid secondDroid = new Fighter("Second", 9, 9, 2, 1, true);
	Droid thirdDroid = new Fighter("Third", 10, 10, 2, 1, false);
	Droid forthdDroid = new Fighter("Forth", 9, 9, 2, 1, false);
	
	
	List<Droid> firstTeam;
	List<Droid> secondTeam;
	
	public TeamOfDroids(){
		firstTeam = new LinkedList<>();
		//firstTeam.add(firstDroid);
		firstTeam.add(thirdDroid);
		secondTeam = new LinkedList<>();
		secondTeam.add(secondDroid);
		secondTeam.add(forthdDroid);
	}

	public List<Droid> getFirstTeam() {
		return firstTeam;
	}

	public List<Droid> getSecondTeam() {
		return secondTeam;
	}

}
