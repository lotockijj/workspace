package task2;

import org.junit.Assert;
import org.junit.Test;

public class PlateauTest {

	@Test
	public void testSolution() {
		int[] arr = {1, 2, 2, 1, 4, 5, 5, 5, 2, 10, 2, 22};
		int[] result = Plateau.solution(arr);
		int[] expected = {4, 3, 8};
		Assert.assertArrayEquals(expected, result);
	}

}
