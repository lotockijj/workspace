package designpatterns;

public class Client {
	
	public static void main(String[] args) {
		SalaryCalculator worker = new Worker("plumber", 5, 8);
		SalaryCalculator owner = new Owner("GreenGrocer", 5, 8);
		
		Person person1 = new Person("John", worker);
		Person person2 = new Person("Mary", owner);
		
		System.out.println(person1);
		System.out.println(person2);
	}
}
