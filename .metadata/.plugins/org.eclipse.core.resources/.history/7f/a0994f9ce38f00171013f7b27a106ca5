package designpatterns.fiftinth;

import java.math.BigDecimal;
import java.util.Vector;

public class Product extends ProductComponent {
	Vector<ProductComponent> productContext;
	private String name;
	private BigDecimal price;
	
	public Product(String name, BigDecimal price) {
		super(price);
		this.name = name;
		productContext = new Vector<>();
	}
	
	@Override
	public void addComponent(ProductComponent product){
		productContext.add(product);
	}
	
	@Override
	public BigDecimal getPrice() {
		return null;
	}

}
