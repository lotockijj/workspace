package designpatterns.fourteen;

import org.junit.Assert;

public class Test {
	
	public static void main(String[] args) {
		FactoryBuilder factory = new FactoryBuilder();
		Builder builderPacket = factory.getBuilder("Packet");
		Container container = new Container(builderPacket);
		container.build();
		Assert.assertEquals("AB packet", 
				builderPacket.getObject().toString());
		
		Builder builderBox = factory.getBuilder("Box");
		Container container2 = new Container(builderBox);
		container2.build();
		Assert.assertEquals("CD box", builderBox.getObject().toString());
	}
}
