package designpatterns;

public class Person {
	private String name;
	private SalaryCalculator salary;

	public Person(String name, SalaryCalculator salary) {
		this.name = name;
		this.salary = salary;
	}

	@Override
	public String toString() {
		return "Person [name=" + name + ", salary=" + salary + "]" + " | " + salary.toString() + salary.getSalary();
	}
	
	
}
