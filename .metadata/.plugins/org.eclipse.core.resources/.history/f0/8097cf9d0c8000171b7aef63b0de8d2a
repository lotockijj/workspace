package designpatterns.first;

import java.util.Arrays;

public class Client {
	
	public static void main(String[] args) {
		int[] arr = {2, 4, 5, 1, 3, 8, 100, 25};
		
		Search search = new LinearSearch();
		System.out.println(search.getItem(arr, 100));
		
		search = new BinarySearch();
		Arrays.sort(arr);
		System.out.println(search.getItem(arr, 6));
	} 

}
